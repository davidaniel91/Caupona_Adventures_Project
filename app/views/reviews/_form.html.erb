<%= form_for([@restaurant, @restaurant.reviews.build]) do |f| %>

    
<p>
    <%= f.label :commenter %><br>
    <%= f.text_field :commenter %>
</p>


<p>
    <%= f.label :review %><br>
    <%= f.text_field :review %>
</p>

<p>
    <%= f.label :rating %><br>
    <%= f.text_area :rating %>
</p>

<p>
    <%= f.submit %>
</p>
<% end %>