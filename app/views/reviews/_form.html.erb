<%= form_for([@restaurant, @restaurant.reviews.build]) do |f| %>

    
<p>
    <%= f.label :commenter %><br>
    <%= f.text_field :commenter %><br>
    <%= f.label :rating %><br>
    <%= f.text_field :rating %><br>
    <%= f.label :review %><br>
    <%= f.text_area :review %>

</p>

<p>
    <%= f.submit %>
</p>
<% end %>